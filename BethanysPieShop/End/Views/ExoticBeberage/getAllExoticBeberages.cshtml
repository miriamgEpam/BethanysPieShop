@model BethanysPieShop.ViewModels.ExoticBeberageVM

<h2> @Model.Title</h2>

@foreach (var eb in Model.ExoticBeberages)
{
    <div class="col-sm-4 col-lg-4 col-md-4">
        <div class="thumbnail">
            <img src="data:image/jpeg;base64," alt="">
            <div class="caption">
                <h3 class="pull-right">@eb.Costo.ToString("c")</h3>
                <h3>
                       <a asp-controller="ExoticBeberage" asp-action="DetailsExoticBeberage" asp-route-id="@eb.ExoticBeberageId">@eb.Description</a>

                </h3>
                <p>@eb.Description</p>
                Paquetes: @eb.Paquetes
                Piezas:@eb.PiezasPorPaquetes
            </div>
        </div>
    </div>

}
